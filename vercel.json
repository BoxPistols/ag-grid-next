{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "when": {
        "value": "next",
        "env": "BUILD_ENV"
      }
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "storybook-static"
      },
      "when": {
        "value": "storybook",
        "env": "BUILD_ENV"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "storybook-static/$1",
      "when": {
        "value": "storybook",
        "env": "BUILD_ENV"
      }
    }
  ]
}